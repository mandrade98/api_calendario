generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String    @id @default(uuid())
  full_name     String
  birth_date    DateTime? @db.Date
  email_address String    @unique
  created_at    DateTime  @default(now()) @db.Timestamptz(3)

  @@map("users")
}
